_extraxtCommonFiles() {
  local -a files
  files=(${SCRIPT_DIR:-$(dirname $words[1])}/commonSongs/*.mulu(N:t))
  _describe 'file' files
}
compdef _extraxtCommonFiles extraxtCommonFiles.sh

_uploadCommonSongs() {
  local -a files
  files=(${SCRIPT_DIR:-$(dirname $words[1])}/../public/commonSongs/*.mulu(N:t))
  _describe 'file' files
}
compdef _uploadCommonSongs uploadCommonSongs.sh